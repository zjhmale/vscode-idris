{
  "name": "vscode-idris",
  "displayName": "idris Syntax Highlighting",
  "description": "Syntax highlighting and snippets for the idris programming language.",
  "version": "1.0.0",
  "publisher": "zjhmale",
  "engines": {
    "vscode": "^0.10.1"
  },
  "icon": "images/idris.png",
  "galleryBanner": {
    "color": "#00ff99",
    "theme": "light"
  },
  "keywords": [
    "language",
    "idris",
    "highlight",
    "syntax",
    "snippets"
  ],
  "license": "BSD",
  "homepage": "https://github.com/zjhmale/vscode-idris",
  "repository": {
    "type": "git",
    "url": "https://github.com/zjhmale/vscode-idris.git"
  },
  "bugs": {
    "url": "https://github.com/zjhmale/vscode-idris/issues",
    "email": "zjhmale@gmail.com"
  },
  "categories": [
    "Languages",
    "Snippets"
  ],
  "activationEvents": [
    "onLanguage:idris"
  ],
  "main": "./src/extension",
  "contributes": {
    "languages": [{
      "id": "idris",
      "aliases": [
        "Idris",
        "idris"
      ],
      "extensions": [
        ".idr",
        ".lidr"
      ]
    }],
    "grammars": [{
      "language": "idris",
      "scopeName": "source.idris",
      "path": "./syntaxes/idris.tmLanguage"
    }],
    "commands": [{
      "command": "idris.typecheck",
      "title": "Idris: Typecheck File"
    }],
    "snippets": [{
      "language": "idris",
      "path": "./snippets/idris.json"
    }]
  },
  "scripts": {
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "dependencies": {
    "bennu": "17.3.0",
    "nu-stream": "3.3.1",
    "rx-lite": "4.0.0",
    "@cycle/core": "3.1.0",
    "@cycle/dom": "5.3.0"
  },
  "devDependencies": {
    "eslint": "^3.4.0",
    "eslint-config-standard": "^6.0.0",
    "eslint-plugin-promise": "^2.0.1",
    "eslint-plugin-standard": "^2.0.0",
    "vscode": "^0.11.0"
  }
}
